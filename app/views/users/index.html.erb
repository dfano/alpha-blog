<h1 align="center">Users</h1>
<table class="container">
	<tr>
		<th>id</th>
		<th>username</th>
		<th>email</th>
		<th>Edit</th>
	</tr>
	<% @users.each do |user| %>

	<tr>
		<td><%= link_to user.id, user_path(user) %></td>
		<td><%= user.username %></td>
		<td><%= user.email %></td>
		<td><%= link_to 'edit', edit_user_path(user) %></td>
	</tr>
	<% end %>

</table>
